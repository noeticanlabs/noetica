{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CK0 Receipt",
  "type": "object",
  "required": [
    "step_id",
    "rule_id",
    "input_hash",
    "output_hash",
    "V_k",
    "V_k1",
    "dV_k",
    "B_k",
    "D_k",
    "D_k1",
    "prev_receipt_hash",
    "receipt_hash"
  ],
  "properties": {
    "step_id": {"type": "integer", "minimum": 0},
    "rule_id": {"type": "string", "minLength": 1},
    "input_hash": {"type": "string", "pattern": "^[0-9a-f]{64}$"},
    "output_hash": {"type": "string", "pattern": "^[0-9a-f]{64}$"},
    "V_k": {"type": "number", "minimum": 0},
    "V_k1": {"type": "number", "minimum": 0},
    "dV_k": {"type": "number", "minimum": 0},
    "B_k": {"type": "number", "minimum": 0},
    "D_k": {"type": "number", "minimum": 0},
    "D_k1": {"type": "number", "minimum": 0},
    "prev_receipt_hash": {"type": "string", "pattern": "^[0-9a-f]{64}$"},
    "receipt_hash": {"type": "string", "pattern": "^[0-9a-f]{64}$"},
    "timestamp": {"type": "string"}
  },
  "additionalProperties": false
}
